{
  "name": "stock-info-collector-backend",
  "version": "1.0.0",
  "description": "Stock investment analysis backend with AI-powered recommendations",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:seed": "node seed.js",
    "db:fix": "node fix-db.js",
    "test": "vitest",
    "test:api": "node quick-test.js",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "stock",
    "investment",
    "ai",
    "analysis"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/cors": "^8.5.0",
    "@fastify/helmet": "^9.1.0",
    "@fastify/rate-limit": "^8.1.1",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^1.10.2",
    "@huggingface/inference": "^4.5.1",
    "@prisma/client": "6.11.1",
    "@types/node": "^24.0.13",
    "axios": "^1.10.0",
    "dotenv": "^17.2.0",
    "fastify": "^4.29.1",
    "moment": "^2.30.1",
    "node-cron": "^4.2.1",
    "nodemon": "^3.1.10",
    "prisma": "6.11.1",
    "technicalindicators": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.2.5",
    "vitest": "^3.2.4"
  }
}
